<!DOCTYPE html>
<html>
<head>
    <title>{{ project_name }} documentation</title>
    <style>{{ source('@EasyDoc/assets/all.css') }}</style>
    <style>
        body {
            display: flex;
            font-size: 14px;
        }

        .nowrap { white-space: pre; }

        .navbar {
            background-color: #444;
        }
        .navbar-brand {
            color: #fff;
            font-size: 1.1rem;
            font-weight: 600;
            margin: 0;
            padding: .15rem 0;
        }
        aside {
            background-color: #f4f4f4;
            border-right: 1px solid #eee;
            overflow-x: hidden;
            overflow-y: auto;
            flex: 0 0 180px;
        }
        aside nav {
            position: fixed;
            top: 60px;
        }
        aside .nav-link {
            color: #292b2c;
        }
        article {
            margin-left: 30px;
            position: relative; /* needed for 'scroll spy' */
            flex: 0 0 960px;
        }
        article section {
            margin-bottom: 1280px;
            padding-top: 51px;
        }
        footer {
            position: absolute;
            bottom: 15px;
            color: #bbb;
            font-size: 10px;
            left: 15px;
            width: 160px;
        }

        h1.page-title {
            border-bottom: 1px solid #ddd;
            color: #444;
            font-size: 1.5rem;
            font-weight: 500;
            margin-bottom: 15px;
            margin-top: 15px;
            padding-bottom: 5px;
        }

        ul.unstyled { list-style: none; padding-left: 0; }

        .table thead th,
        .table td, .table th {
            border: 1px solid #ddd;
        }
        .table thead th {
            border-bottom-width: 2px;
            background-color: #fafafa;
        }
        .table a + a { margin-left: 1em; }
        .table + .table { margin-top: 2rem; }
        .table tbody th { vertical-align: top; width: 15%; }
        .table h3 { font-size: 1rem; font-weight: 500; margin: 0; }
        .markdown-body table tbody td, .code { font-family: Ubuntu Mono, Consolas, Inconsolata, Monaco, monospace; word-break: break-all; }
        .table tbody td ul { margin-bottom: 0; }
        .table tbody td span + b { margin-left: 1rem; }
        .table tbody td ul + b { display: inline-block; margin-top: 1rem; }

        .badge {
            color: #999;
            display: inline-block;
            border: 2px solid #ccc;
            font-weight: 500;
            min-width: 47px;
            padding: 3px 6px;
        }
        .badge-prod {
            border-color: #5cb85c;
            color: #5cb85c;
        }

        .blankslate {
            border: 2px dashed #eee;
            border-radius: 4px;
            color: #aaa;
            margin-top: 30px;
            padding: 90px;
            text-align: center;
            width: 100%;
        }
        .blankslate h3 { font-size: 1.25rem; }

        .card-deck { margin-top: 30px; }
        .card { max-width: 140px; text-align: center;}
        .card-title {
            color: #555;
            font-size: 1.75rem;
            margin-bottom: .25rem;
        }
        .card-score { max-width: 310px; }
        .card-score .card-title { font-size: 3.5rem; }
        .card-score .card-text { font-size: 1rem; }
        .card-score .card-text a { font-size: .8rem; }
    </style>
    <script>{{ source('@EasyDoc/assets/all.js') }}</script>
</head>
<body>
    <nav class="navbar fixed-top">
        <h1 class="navbar-brand">{{ project_name }}</h1>
    </nav>

    <aside>
        <nav class="nav flex-column">
            <a class="nav-link" href="#overview">Project Overview</a>
            <a class="nav-link" href="#routes">Routes</a>
            <a class="nav-link" href="#services">Services</a>
            <a class="nav-link" href="#events">Events</a>
            <a class="nav-link" href="#security">Security</a>
            <a class="nav-link" href="#translation">Internationalization</a>
            <a class="nav-link" href="#templates">Templates</a>
            <a class="nav-link" href="#doctrine">Doctrine Entities</a>
            <a class="nav-link" href="#configuration">Configuration</a>
            <a class="nav-link" href="#bundles">Bundles</a>
            <a class="nav-link" href="#licenses">Licenses</a>
        </nav>
    </aside>

    <article data-spy="scroll" data-target="aside" data-offset="0">
        <section id="overview">
            <h1 class="page-title">Project Overview</h1>
            {{ include('@EasyDoc/_overview.html.twig') }}
        </section>

        <section id="routes">
            {{ include('@EasyDoc/_routes.html.twig') }}
        </section>

        <section id="services">
            {{ include('@EasyDoc/_services.html.twig') }}
        </section>

        <section id="events">
            <h1 class="page-title">Events</h1>
            {{ include('@EasyDoc/_unfinished_panel.html.twig') }}
        </section>

        <section id="security">
            <h1 class="page-title">Security</h1>
            {{ include('@EasyDoc/_unfinished_panel.html.twig') }}
        </section>

        <section id="translation">
            <h1 class="page-title">Internationalization</h1>
            {{ include('@EasyDoc/_unfinished_panel.html.twig') }}
        </section>

        <section id="templates">
            <h1 class="page-title">Templates</h1>
            {{ include('@EasyDoc/_unfinished_panel.html.twig') }}
        </section>

        <section id="doctrine">
            <h1 class="page-title">Doctrine Entities</h1>
            {{ include('@EasyDoc/_unfinished_panel.html.twig') }}
        </section>

        <section id="configuration">
            <h1 class="page-title">Configuration</h1>
            {{ include('@EasyDoc/_unfinished_panel.html.twig') }}
        </section>

        <section id="bundles">
            {{ include('@EasyDoc/_bundles.html.twig') }}
        </section>

        <section id="licenses">
            {{ include('@EasyDoc/_licenses.html.twig') }}
        </section>
    </article>

    <footer>
        Built with EasyDoc {{ easydoc_version }}
        on {{ last_build_date|date }}
    </footer>

    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>
</body>
</html>
