{
    "name": "easycorp/easy-doc-bundle",
    "type": "symfony-bundle",
    "description": "Symfony application documentation generator",
    "keywords": ["documentation", "generator", "project"],
    "homepage": "https://github.com/EasyCorp/EasyDocBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Eguiluz",
            "email": "javiereguiluz@gmail.com"
        }
    ],
    "require": {
        "php"                           : ">=7.1.3",
        "symfony/dependency-injection"  : "^4.2",
        "symfony/framework-bundle"      : "^4.2",
        "symfony/twig-bridge"           : "^4.2",
        "symfony/twig-bundle"           : "^4.2",
        "twig/extensions"               : "^1.5",
        "twig/twig"                     : "^2.7"
    },
    "require-dev": {
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "EasyCorp\\Bundle\\EasyDocBundle\\": "src/" },
        "exclude-from-classmap": []
    },
    "autoload-dev": {
    }
}
