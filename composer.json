{
    "name": "easycorp/easy-doc-bundle",
    "type": "symfony-bundle",
    "description": "Symfony application documentation generator",
    "keywords": ["documentation", "generator", "project"],
    "homepage": "https://github.com/EasyCorp/EasyDocBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Eguiluz",
            "email": "javiereguiluz@gmail.com"
        }
    ],
    "require": {
        "php"                           : ">=5.3.3",
        "symfony/dependency-injection"  : "~2.3|~3.0",
        "symfony/framework-bundle"      : "~2.3|~3.0",
        "symfony/twig-bridge"           : "^2.3.4|~3.0",
        "symfony/twig-bundle"           : "~2.3|~3.0",
        "twig/extensions"               : "~1.0",
        "twig/twig"                     : "~1.14,>=1.14.2|~2.0",
        "symfony/console"               : "~2.8|~3.0",
        "doctrine/doctrine-bundle"      : "~1.4",
        "doctrine/orm"                  : "^2.4.8"

    },
    "require-dev": {
    },
    "config": {
        "platform": {
            "php": "5.3.9"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "EasyCorp\\Bundle\\EasyDocBundle\\": "src/" },
        "exclude-from-classmap": []
    },
    "autoload-dev": {
    }
}
